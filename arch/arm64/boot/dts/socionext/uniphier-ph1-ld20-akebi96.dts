/dts-v1/;
#include "uniphier-ph1-ld20.dtsi"

/ {
	model = "Akebi96 Development Board";
	compatible = "socionext,ph1-ld20-akebi96", "socionext,ph1-ld20";

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs = "earlycon";
	};

	aliases {
		serial0 = &serial0;
		serial1 = &serial1;
		serial2 = &serial2;
		serial3 = &serial3;
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		i2c4 = &i2c4;
		i2c5 = &i2c5;
	};

	memory {
		device_type = "memory";
		reg = <0 0x80000000 0 0xc0000000>;
	};
};

&serial0 {
	status = "okay";
};

&serial3 {
	status = "okay";
};

&emmc {
	status = "okay";
};

&usb {
	status = "okay";
};

&eth {
	status = "okay";
	phy-handle = <&ethphy>;
};

&mdio {
	ethphy: ethphy@0 {
		reg = <0>;
	};
};

&pcie {
	status = "okay";
};

&spi3 {
	status = "okay";
};
